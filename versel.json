{
    "builds": [{
        "src": "bookmymovie/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.6" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "bookmymovie/wsgi.py"
        }
    ],
    "REWRITES":[
        {
            "source": "/(.*)",
            "destination": "/bookmymovie/wsgi.py"
        }
    ]
}
